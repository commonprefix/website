{
	"framework": null,
	"outputDirectory": "public",
	"cleanUrls": true,
    "headers": [
      {
        "source": "/.well-known/xrp-ledger.toml",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/projects/:path*",
        "destination": "/clients/:path*",
        "permanent": true
      },
      {
        "source": "/static/projects/:path*",
        "destination": "/static/clients/:path*",
        "permanent": true
      }
    ]
}
